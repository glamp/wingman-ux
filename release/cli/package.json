{
  "name": "wingman-cli",
  "version": "1.0.0",
  "description": "CLI for Wingman - Lightweight UX feedback assistant with local relay server",
  "keywords": ["wingman", "ux", "feedback", "cli", "developer-tools"],
  "author": "Wingman Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wingman/wingman.git",
    "directory": "packages/cli"
  },
  "main": "dist/index.js",
  "bin": {
    "wingman": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "coverage": "npm run test:coverage"
  },
  "dependencies": {
    "@wingman/relay-server": "*",
    "@wingman/shared": "*",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@types/node": "^22.12.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

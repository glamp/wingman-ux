{
  "name": "@wingman/tunnel-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Tunnel server for Wingman P2P connections",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:production": "vitest src/__tests__/production.test.ts --run",
    "test:local": "vitest src/__tests__/api.test.ts src/__tests__/session-manager.test.ts --run",
    "test:e2e": "node test-production-automated.js",
    "test:e2e:verbose": "node test-production-automated.js --verbose",
    "test:all": "npm run test -- --run && npm run test:e2e"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "cookie-parser": "^1.4.6",
    "multer": "^1.4.5-lts.1",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}

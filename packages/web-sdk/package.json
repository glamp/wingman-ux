{
  "name": "wingman-sdk",
  "version": "1.0.0",
  "description": "Wingman Web SDK - React integration for UX feedback collection",
  "keywords": ["wingman", "react", "sdk", "ux", "feedback", "developer-tools"],
  "author": "Wingman Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/glamp/wingman-ux.git",
    "directory": "packages/web-sdk"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "coverage": "npm run test:coverage"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}